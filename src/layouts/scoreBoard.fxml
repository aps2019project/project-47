<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.control.ScrollPane?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controllers.graphical.ScoreBoardController"
            prefHeight="1080.0" prefWidth="1920">
    <ScrollPane layoutX="860.0" layoutY="92.0" prefHeight="200.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0"
                AnchorPane.topAnchor="0.0">
        <content>
            <VBox fx:id="battle" prefWidth="600.0">
                <children>
                    <HBox prefHeight="100.0" prefWidth="600.0"/>
                </children>
            </VBox>
        </content>
    </ScrollPane>
    <ImageView fitHeight="150.0" fitWidth="150.0" onMouseClicked="#back">
        <Image url="/resources/buttons/back-button.png"/>
    </ImageView>
</AnchorPane>
